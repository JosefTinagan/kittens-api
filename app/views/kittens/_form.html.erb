<%= form_for(@kitten) do |f| %>
	<%= f.label :name %>
	<%= f.text_field :name %>

	<br>
	<%= f.label :age %>
	<%= f.number_field(:age, in: 1..25) %>

	<br>
	<%= f.label :cuteness %>
	<%= f.number_field(:cuteness, in: 1..10) %>

	<br>
	<%= f.label :softness %>
	<%= f.number_field(:softness, in: 1..10) %>

	<br>
	<%= f.submit yield(:submit) %>
<% end %>