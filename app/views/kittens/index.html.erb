<h1>Kitten Index</h1>

<ul>
	<p>List of Kittens:</p>
	<% @kittens.each do |kitten| %>
		<li>
			<p>Name: <%= kitten.name %> </p>
			<p>Age: <%= kitten.age %></p>
			<p>Cuteness: <%= kitten.cuteness %></p>
			<p>Softness: <%= kitten.softness %></p>
			<p><%= link_to "delete", kitten, method: :delete, data: { confirm: "You sure?" } %></p>
		</li>
	<% end %>
</ul>